{
  "name": "zoning-dn",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "parcel src/index.html --global script",
    "build": "parcel build src/index.html --global script --public-url https://spectator-static-assets.s3.amazonaws.com/zoning-dn --no-content-hash",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "cssnano": "^4.1.10",
    "eslint": "^7.0.0",
    "parcel-bundler": "1.12.3",
    "patch-package": "^6.2.2",
    "posthtml-expressions": "1.4.0",
    "posthtml-include": "^1.4.3",
    "sass": "^1.26.5",
    "@babel/helper-create-regexp-features-plugin": "^7.21.8",
    "@babel/core": "^7.21.8"
  },
  "dependencies": {
    "intersection-observer": "^0.10.0",
    "text-balancer": "^1.0.5"
  },
  "browserslist": [
    "defaults"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid"
  },
  "spectate": {
    "USE_NEWS_NAV": true,
    "USE_EYE_NAV": false,
    "USE_COVER_HED": true,
    "IS_EMBED": false,
    "DOC_URL": "https://docs.google.com/document/d/12hcVokHqBAEvWEPXzqG4fEXQ1E-t_3EvWkHeRFKz5I0/edit"
  }
}
